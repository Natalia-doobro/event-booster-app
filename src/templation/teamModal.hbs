<div class='content'>
  <button class='close'></button>
  {{#each this}}
    <figure class='modal-foto'>
      <img src='{{photo}}' class='background' />
      <img src='{{photo}}' alt={{fullName}} class='profile' />
      <figcaption>
        <h3 class="modal-title">{{fullName}}<p class="modal-text">{{position}}</p></h3>
       <div class='icons'>
          <a href=""  aria-label="" target="_blank"
                          rel="noopener noreferrer">
                          <ion-icon name="logo-slack" class="
                            ion-icon"></ion-icon>
          </a>
          <a href=""  aria-label="" target="_blank"
                          rel="noopener noreferrer">
                           <ion-icon name="logo-linkedin" class="
                            ion-icon "></ion-icon>
                          </a>
                           <a href=""  aria-label="" target="_blank"
                          rel="noopener noreferrer">
                      <ion-icon name="logo-github" class="
                            ion-icon"></ion-icon>
                      </a>


        </div>
      </figcaption>
    </figure>
  {{/each}}
</div>